                 <%= form.text_field :password_confirmation,  class: 'form-control',id: 'venuetextbox1', placeholder: 'password confirmation' %>
              </div>
              <div class="form-group">
                 <%= form.text_field :name,  class: 'form-control',id: 'venuetextbox1', placeholder: 'Name' %>
