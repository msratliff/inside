<h1>Log In</h1>

<%= form_tag venues_sessions_path do %>
	<p>
		<%= label_tag :email %>
		<%= text_field_tag :email %>
	</p>

	<p>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</p>

	<p>
		<%= submit_tag 'Log In' %>
	</p>

<% end %>

<%= link_to 'Create Venue Account', new_venue_path %>
<%= link_to 'User Login', users_login_path %>